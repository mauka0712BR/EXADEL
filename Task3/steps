mkdir /opt/exadel/ubuntu    
mkdir /opt/exadel/centos  
cd /opt/exadel/ubuntu  
export AWS_ACCESS_KEY_ID=***  
export AWS_SECRET_ACCESS_KEY=***  
terraform init  
terraform plan >> ubuntu.plan  
terraform apply  
cd /opt/exadel/centos    
terraform init  
terraform plan >> centos.plan  
terraform apply  
